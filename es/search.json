---
layout: none
---

[
  {% for post in site.es | sort: 'date' | reverse %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.categories }}",
      "url"      : "{{ post.url }}",
      "date"     : "{{ post.date | date: "%d/%m/%Y" }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
